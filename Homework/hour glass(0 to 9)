#include <iostream>
#include<cmath>

using namespace std;
int main() {
    for(int i = 0; i < 19; i++)
    {
        for(int j = 0; j < 19; j++)
        {
            int column =  9 - abs(9 - j);
            int row =  9 - abs(9 - i);
            if (column - row >= 0)
            {
                cout<<int(column - row )<<" ";
            }
            else
            {
                cout<<"  ";
            }
        }
        cout<<endl;
        
    }
    return 0;
}  
